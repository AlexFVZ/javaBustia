CREATE TABLE IF NOT EXISTS Preu(
preu TEXT,
diaSessio TEXT,
codiZona INTEGER,
FOREIGN KEY (diaSessio) REFERENCES Sessio(dia),
FOREIGN KEY (codiZona) REFERENCES Zona(codi)
);

select:
SELECT * FROM Preu;

insertAmbPreu:
INSERT INTO Preu(preu,diaSessio,codiZona) VALUES (?,?,?);

insertSensePreu:
INSERT INTO Preu(diaSessio,codiZona) VALUES (?,?);